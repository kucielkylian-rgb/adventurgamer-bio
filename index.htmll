<!-- Contrôles musique -->
    <div class="music-controls">
        <button class="music-btn" onclick="toggleMusic()">🎵</button>
        <audio id="bgMusic" loop>
            <!-- Tu devras remplacer cette URL par le fichier Silent Reverie -->
            <source src="https://www.bensound.com/bensound-music/bensound-energy.mp3" type="audio/mpeg">
            <source src="c:\Users\PC\Desktop\Nouveau dossier\Nouveau dossier\0EM9eE3rVXW16Erg3yFTdo.url" type="audio/mpeg">
        </audio>
    </div>

<script>
        // Contrôle de la musique
        let musicPlaying = false;
        function toggleMusic() {
            const music = document.getElementById('bgMusic');
            const btn = document.querySelector('.music-btn');
            
            if (musicPlaying) {
                music.pause();
                btn.innerHTML = '🎵';
                musicPlaying = false;
            } else {
                music.play();
                btn.innerHTML = '⏸';
                musicPlaying = true;
            }
        }
    </script>

    <style>
        .music-controls {
            position: fixed;
            top: 20px;
            right: 20px;
            z-index: 1000;
            background: rgba(255, 255, 255, 0.2);
            padding: 10px;
            border-radius: 10px;
            border: 2px solid rgba(255, 255, 255, 0.3);
            backdrop-filter: blur(10px);
        }

        .music-btn {
            background: linear-gradient(45deg, #60a5fa, #3b82f6);
            border: none;
            padding: 10px;
            border-radius: 50%;
            cursor: pointer;
            font-size: 18px;
            color: white;
            box-shadow: 0 5px 15px rgba(96, 165, 250, 0.3);
            transition: transform 0.3s ease;
        }

        .music-btn:hover {
            transform: scale(1.1);
        }
    </style>
    
        
